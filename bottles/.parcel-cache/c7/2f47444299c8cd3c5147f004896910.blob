    <style data-parcel-key="76d0d3b5f2142c033cc275a09106c634">
        body {
            font-family: Helvetica Neue, Helvetica, sans-serif;
            margin: 0;
            padding: 0;
            font-size: 14px
        }

        header {
            background-color: #999;
            color: #eee;
            height: 0
        }

        main {
            background-color: #333;
            color: #ddd;
            height: 100vh;
            overflow: scroll
        }

        footer {
            background-color: #666;
            color: #eee;
            height: 0
        }

        #bottles {
            position: absolute;
            top: 0px;
            width: 100vw;
            height: 100vh;
            left: 0px;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .results {
            position: absolute;
            top: -400px;
            left: 100px;
            width: 100vw;
            height: 400px;
        }

        .assembled {
            position: absolute;
            top: 0px;
            left: 0px;
            perspective: 1500px;
            overflow: visible;

        }

        #bottles img {}

        .assembled img {
            animation: spin 800ms linear infinite;
            transition: all 5000ms cubic-bezier(0.16, 1, 0.3, 1);
        }

        @keyframes spin {
            0% {
                transform: rotate3d(0, 1, 0, 0deg);
            }

            100% {
                transform: rotate3d(.5, -1.5, .5, 720deg);
            }
        }
    </style>
</head>

